# vim: set ft=sxhkdrc:
#

# Tilix
ctrl + alt + t
    tilix

# Screen Shoot parte
XF86ScreenSaver
    gnome-screenshot -a

# Screen Shoot parte
shift + XF86ScreenSaver
    gnome-screenshot -c

# Lynx Launcher
ctrl + shift + space
    /usr/share/applications/org.lynx.desktop.toggleSearch.desktop

# Lynx Menu
super
    /usr/share/applications/org.lynx.desktop.toggleLauncher.desktop

# Nautilus
super + e
    nautilus
