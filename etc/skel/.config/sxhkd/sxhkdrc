# vim: set ft=sxhkdrc:
#

# Tilix
ctrl + alt + t
    tilix

# Screen Shoot parte
impr pant
    gnome-screenshot -a

# Screen Shoot parte
shift + impr pant
    gnome-screenshot -c

# Lynx Launcher
ctrl + shift + space
    /usr/share/applications/org.lynx.desktop.toggleSearch.desktop

# Lynx Menu
super
    /usr/share/applications/org.lynx.desktop.toggleLauncher.desktop
